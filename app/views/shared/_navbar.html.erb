  <% end %>

  <ul class="">
    <% if user_signed_in? %>
      <li class="nav-item">
        <%= link_to new_supplement_path, class:"btn btn-primary" do %>
          <i class="fa-solid fa-plus"></i>
        <% end %>
      </li>
      <li class="nav-item dropdown">
        <%= image_tag "https://res.cloudinary.com/dach7c7ct/image/upload/v1669220890/Alex_avatar_ytczjp.png", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
